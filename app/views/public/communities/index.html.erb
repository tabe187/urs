<%= link_to "コミュニティ新規", new_community_path %><br>
コミュニティ一覧<br>
<% @communities.each do |community| %>
  <% if community.community_image.attached? %>
    <%= link_to community_path(community.id) do %>
       <%= image_tag community.get_community_image(100,100) %><br>
    <% end %><br>
  <% else %>
    <%= link_to community_path(community.id) do %>
       <%= image_tag 'no_image', size: "100x100" %><br>
    <% end %><br>
  <% end %>
<%= community.title %><br>
<%= community.category.name %><br>
<% end %>